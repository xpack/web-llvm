{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "profile": "npm run clear && DOCUSAURUS_RSPACK_TRACE=true DOCUSAURUS_EXIT_AFTER_BUNDLING=true npm run build",
    "docusaurus": "docusaurus",
    "start": "node --max-old-space-size=16000 --stack-size=2048 ./node_modules/.bin/docusaurus start",
    "build": "node --max-old-space-size=16000 --stack-size=2048 ./node_modules/.bin/docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "generate-doxygen": "node --max-old-space-size=8192 --stack-size=2048 ./node_modules/.bin/docusaurus generate-doxygen",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.8.1",
    "@docusaurus/preset-classic": "3.8.1",
    "@docusaurus/faster": "3.8.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.8.1",
    "@docusaurus/tsconfig": "3.8.1",
    "@docusaurus/types": "3.8.1",
    "@xpack/docusaurus-plugin-doxygen": "0.4.0",
    "react-markdown": "^10.1.0",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
